<Window x:Class="StadontwikkelingGentUI.PartnerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StadontwikkelingGentUI"
        mc:Ignorable="d"
        Title="PartnerWindow" Height="450" Width="800">
    <Grid>
        <DataGrid Name="DataGridPartners" Margin="5,5,5,59" SelectionMode="Single" IsReadOnly="True" Grid.RowSpan="1">
            <DataGrid.Resources>
                <Style TargetType="{x:Type DataGridRow}">

                </Style>
            </DataGrid.Resources>
        </DataGrid>
        <Button Content="Verwijder Partner" Margin="510,380,10,14" Click="VerwijderPartner_Click"/>
        <Button Content="Voeg Partner toe" Margin="10,380,510,14" Click="VoegPartnerToe_Click"/>

    </Grid>
</Window>
